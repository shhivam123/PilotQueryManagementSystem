      <form method="GET" action="/watch">
        <div class="inner-form">
          <div class="basic-search">
            <div class="input-field">
              <div class="icon-wrap">
                <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="20" height="20" viewBox="0 0 20 20">
                  <path d="M18.869 19.162l-5.943-6.484c1.339-1.401 2.075-3.233 2.075-5.178 0-2.003-0.78-3.887-2.197-5.303s-3.3-2.197-5.303-2.197-3.887 0.78-5.303 2.197-2.197 3.3-2.197 5.303 0.78 3.887 2.197 5.303 3.3 2.197 5.303 2.197c1.726 0 3.362-0.579 4.688-1.645l5.943 6.483c0.099 0.108 0.233 0.162 0.369 0.162 0.121 0 0.242-0.043 0.338-0.131 0.204-0.187 0.217-0.503 0.031-0.706zM1 7.5c0-3.584 2.916-6.5 6.5-6.5s6.5 2.916 6.5 6.5-2.916 6.5-6.5 6.5-6.5-2.916-6.5-6.5z"></path>
                </svg>
              </div>
              <input id="v" name="v" type="url" placeholder="Search..." />
 
              <div class="result-count">
                <button class="btn-search">Search</button>
 
              </div>
          </div>
      </div>    
          <br></br> 
          <%if (flag.length > 3) { %>
          <div class="advance-search">
            <span class="desc"><font size="3px" color="green"><%= titles %></font><%= duration_hms %><font color="red">
            </font>
          </span>
             <div class="row">
              <div class="input-field">
                <div class="input-selectimg">
                  <img src=<%= imageBlobUrl %> alt="Girl in a jacket" width="100%" height="100%">

                </div>
              </div>
              <div class="input-field">
                <div class="input-select">
                  <select data-trigger="" name="choices-single-defaul" id="mySelect">
                    <option placeholder="" value="">Select Formate</option>
                    <% for (var i =1; i <FORMATE_.length;  i++ ) { %>
                      <option value=<%= URL_[i] %>><%= FORMATE_[i] %></option>
                      <% } %>
                    </select>
                </div>
              </div>
              <div class="input-field">
                <div class="input-select">
                  <button class="btn-search" id="download" target="_blank">DownLoad</button>
                  <button  class="btn-search" value=<%= audioURL %> id="mp3download">MP3/HD</button>
                </div>
              </div>
              
               
            </div>
             
          </div>
          <% } %>
        </div>
      <!-- </form> -->
  
    </div>
    <script src="js/extention/choices.js"></script>
    <script>
      const customSelects = document.querySelectorAll("select");
      const downloadBtn = document.getElementById('download')
      const mp3downloadBtn = document.getElementById('mp3download')

       const choices = new Choices('select',
      {
        searchEnabled: false,
        removeItemButton: true,
        itemSelectText: '',
      });
      for (let i = 0; i < customSelects.length; i++)
      {
        customSelects[i].addEventListener('addItem', function(event)
        {
          if (event.detail.value)
          {
            let parent = this.parentNode.parentNode
            parent.classList.add('valid')
            parent.classList.remove('invalid')
          }
          else
          {
            let parent = this.parentNode.parentNode
            parent.classList.add('invalid')
            parent.classList.remove('valid')
          }
        }, false);
      }
      downloadBtn.addEventListener("click", function(e)
      {
        e.preventDefault()
      var a=  document.getElementById('mySelect').value;
        // const deleteAll = document.querySelectorAll('.choices__button')
        if(a.length<2)
        alert("Please Select Any Formate");
        else
        window.location.href = a;
      });
      mp3downloadBtn.addEventListener("click", function(e)
      {
        e.preventDefault()
      var a=  document.getElementById('mp3download').value;
         // const deleteAll = document.querySelectorAll('.choices__button')
        window.location.href = a;
      });

      
function show()
{
